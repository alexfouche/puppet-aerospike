<%= scope.lookupvar('aerospike::logging_dirs').join("/*.log ") + "/*.log" %> {
  compress
  dateext
  daily
  rotate 31
  copytruncate
  delaycompress
  notifempty
  missingok
  create 640 <%= scope.lookupvar('aerospike::system_user') %> <%= scope.lookupvar('aerospike::logging_dirs_group') %>
}
